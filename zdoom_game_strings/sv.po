msgid "TXT_RNO1_SCRIPT02_D16676_YOUCA"
msgstr "Du har inte råd med det, god dag till dig!"

# MAP02: Armory.
msgid "TXT_DLG_SCRIPT02_D18192_WELCO"
msgstr "Välkommen. Vad får jag visa dig?"

# 〃
msgid "TXT_RPLY0_SCRIPT02_D18192_ENVIR"
msgstr "Miljödräkt"

# 〃
