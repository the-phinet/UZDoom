# 〃
msgid "TXT_RYES3_SCRIPT33_D4548_THANK"
msgstr "Dziękuję, coś jeszcze?"

# 〃
msgid "TXT_RNO3_SCRIPT33_D4548_YOUCA"
msgstr "Nie stać cię na to, dzień dobry."

# MAP33: Armory
